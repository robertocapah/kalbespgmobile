<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#FFFFFF"
    android:orientation="vertical"
    tools:context="com.kalbenutritionals.app.kalbespgmobile.PushData" >

    <TextView
      android:id="@+id/tv1"
       android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:textAppearance="?android:attr/textAppearanceMedium"
      android:textColor="#000"
      android:text="Purchase Order" />
    
    <TableLayout
        android:id="@+id/tlPO"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

       </TableLayout>
       
     <View
          android:id="@+id/view1"
          android:layout_width="fill_parent"
          android:layout_height="2dp"
          android:background="#000" />
     
    <TextView
      android:id="@+id/tv2"
       android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:textAppearance="?android:attr/textAppearanceMedium"
      android:textColor="#000"
      android:text="GRN" />
    
    <TableLayout
      android:id="@+id/tlGRN"
        android:layout_width="match_parent"
       android:layout_height="wrap_content" >
       </TableLayout>
       
     <View
          android:id="@+id/view2"
          android:layout_width="fill_parent"
          android:layout_height="2dp"
          android:background="#000" />
     
    <TextView
      android:id="@+id/tv3"
       android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:textAppearance="?android:attr/textAppearanceMedium"
      android:textColor="#000"
      android:text="Sales" />
    
    <TableLayout
      android:id="@+id/tlSales"
        android:layout_width="match_parent"
       android:layout_height="wrap_content" >
       </TableLayout>

    <View
          android:id="@+id/view3"
          android:layout_width="fill_parent"
          android:layout_height="2dp"
          android:background="#000" />
     
    <TextView
      android:id="@+id/tv4"
       android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:textAppearance="?android:attr/textAppearanceMedium"
      android:textColor="#000"
      android:text="Pengeluaran" />
    
    <TableLayout
      android:id="@+id/tlPengeluaran"
        android:layout_width="match_parent"
       android:layout_height="wrap_content" >
       </TableLayout>
       
    <View
          android:id="@+id/view4"
          android:layout_width="fill_parent"
          android:layout_height="2dp"
          android:background="#000" />
     
    <TextView
      android:id="@+id/tv5"
       android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:textAppearance="?android:attr/textAppearanceMedium"
      android:textColor="#000"
      android:text="Stock Opname" />
    
    <TableLayout
      android:id="@+id/tlStockOpname"
        android:layout_width="match_parent"
       android:layout_height="wrap_content" >
       </TableLayout>
       
    <View
          android:id="@+id/view5"
          android:layout_width="fill_parent"
          android:layout_height="2dp"
          android:background="#000" />
     
    <TextView
      android:id="@+id/tv6"
       android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:textAppearance="?android:attr/textAppearanceMedium"
      android:textColor="#000"
      android:text="Stock Awal" />
    
    <TableLayout
      android:id="@+id/tlStockAwal"
        android:layout_width="match_parent"
       android:layout_height="wrap_content" >
       </TableLayout>
       
    <View
          android:id="@+id/view6"
          android:layout_width="fill_parent"
          android:layout_height="2dp"
          android:background="#000" />
     
    <TextView
      android:id="@+id/tv7"
       android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:textAppearance="?android:attr/textAppearanceMedium"
      android:textColor="#000"
      android:text="Sales Pack Stock" />
    
    <TableLayout
      android:id="@+id/tlSalesPackStock"
        android:layout_width="match_parent"
       android:layout_height="wrap_content" >
       </TableLayout>
       <TableLayout
      android:id="@+id/tlSalesPackStockLog"
        android:layout_width="match_parent"
       android:layout_height="wrap_content" >
       </TableLayout>
       <View
          android:id="@+id/view7"
          android:layout_width="fill_parent"
          android:layout_height="2dp"
          android:background="#000" />
     
    <TextView
      android:id="@+id/tv8"
       android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:textAppearance="?android:attr/textAppearanceMedium"
      android:textColor="#000"
      android:text="Transaction Detail" />
    
    <TableLayout
      android:id="@+id/tlTransactionDetail"
        android:layout_width="match_parent"
       android:layout_height="wrap_content" >
       </TableLayout>
        <View
          android:id="@+id/view8"
          android:layout_width="fill_parent"
          android:layout_height="2dp"
          android:background="#000" />
     
    <TextView
      android:id="@+id/tv9"
       android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:textAppearance="?android:attr/textAppearanceMedium"
      android:textColor="#000"
      android:text="Absen" />
    
    <TableLayout
      android:id="@+id/tlAbesen"
        android:layout_width="match_parent"
       android:layout_height="wrap_content" >
       </TableLayout>
       <View
          android:id="@+id/view9"
          android:layout_width="fill_parent"
          android:layout_height="2dp"
          android:background="#000" />
     
    <TextView
      android:id="@+id/tv10"
       android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:textAppearance="?android:attr/textAppearanceMedium"
      android:textColor="#000"
      android:text="Leave" />
    
    <TableLayout
      android:id="@+id/tlLeave"
        android:layout_width="match_parent"
       android:layout_height="wrap_content" >
       </TableLayout>

        <Button
            android:id="@+id/btnPush"
             android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|fill"
            android:text="Push Data" />

</LinearLayout>
